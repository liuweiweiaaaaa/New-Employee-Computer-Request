        <script><![CDATA[function onLoad() {
    // // 确保字段可见
    // g_form.setDisplay('upload_attachment', true);
    
    // // 使用 UI 策略监听变化
    // if (g_form.hasField('upload_attachment')) {
    //     // 旧版本兼容方式
    //     g_form.getControl('upload_attachment').onchange = function() {
    //         if (g_form.getValue('upload_attachment')) {
    //             processExcelFile();
    //         }
    //     };
    // }
}

function processExcelFile() {
    try {
        var attachmentSysId = g_form.getValue('upload_attachment');
        if (!attachmentSysId) return;
        
        // 显示处理状态
        g_form.showFieldMsg('正在处理 Excel 文件...', 'info');
        g_form.setReadOnly('upload_attachment', true);
        
        // 使用 GlideAjax
        var ga = new GlideAjax('ExcelToMRVSProcessor');
        ga.addParam('sysparm_name', 'processExcel');
        ga.addParam('sysparm_attachment_sys_id', attachmentSysId);
        ga.addParam('sysparm_mrvs_var_name', 'multi_row_variable_set');
        ga.addParam('sysparm_sys_id', g_form.getUniqueValue());
        ga.getXML(function(response) {
            g_form.setReadOnly('upload_attachment', false);
            
            var answer = response.responseXML.documentElement.getAttribute('answer');
			console.log('```````````````````'+answer);
            if (answer === 'success') {
                g_form.showFieldMsg('Excel 处理成功', 'success');
                g_form.refreshMRVS('multi_row_variable_set');
            } else {
                g_form.showFieldMsg('处理失败: ' + answer, 'error');
                g_form.setValue('upload_attachment', '');
            }
        });
    } catch (e) {
        g_form.showFieldMsg('处理出错: ' + e.message, 'error');
        g_form.setReadOnly('upload_attachment', false);
    }
}]]></script>
