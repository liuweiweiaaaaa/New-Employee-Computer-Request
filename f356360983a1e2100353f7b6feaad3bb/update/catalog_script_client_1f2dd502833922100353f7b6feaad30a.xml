        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading) {
        return;
    }
    if (newValue == '') {
        g_form.clearValue("multi_row_variable_set");
    }

    var ajax = new GlideAjax('ExcelToMRVSProcessor');
    ajax.addParam('sysparm_name', 'addExceldatainMRVS');
    ajax.addParam('sysparm_existing', g_form.getValue('upload_attachment'));
    ajax.getXML(getRITMdata);

    function getRITMdata(response) {
        var answer = response.responseXML.documentElement.getAttribute("answer");
        alert(answer);
        g_form.setValue('multi_row_variable_set', answer); // this is mrvs internal name

    }
}]]></script>
