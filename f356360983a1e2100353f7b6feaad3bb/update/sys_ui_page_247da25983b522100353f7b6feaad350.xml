        <category>general</category>
        <client_script><![CDATA[function onCancel() {
GlideDialogWindow.get().destroy();
return false;
} 

function onSubmit() {
var data = gel('notes').value;
var sysId = gel('task_sys_id');
var gaLoc = new GlideAjax('FieldUpdater');
gaLoc.addParam('sysparm_name','updateField');
gaLoc.addParam('sys_id', sysId);
gaLoc.addParam('sysparm_field_name', 'sn_number');
gaLoc.addParam('sysparm_new_value', data);
gaLoc.getXML(assgnLocSysid);
GlideDialogWindow.get().destroy();
function assgnLocSysid(response) {
  var answer = response.responseXML.documentElement.getAttribute("answer");
  alert(answer);
  //g_form.setValue('sn_number',answer.sn_number);
  GlideDialogWindow.get().destroy();
}
}]]></client_script>
